{
  "name": "react-ts-parcel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && cross-env parcel src/index.html --out-dir public",
    "build": "npm run clean && cross-env parcel build src/index.html --out-dir public --no-minify --public-url /",
    "clean": "cross-env rimraf public .cache",
    "lint:file": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:all": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "pretty": "cross-env fixme=fixme prettier --write '**/*.{j,t}{s,sx}'",
    "pretty:watch": "cross-env fixme=fixme onchange '**/*.{j,t}{s,sx}' -- prettier --write {{changed}}",
    "ts-check": "cross-env tsc",
    "ts-check:watch": "yarn ts-check -- --watch",
    "test": "cross-env jest",
    "test:watch": "cross-env jest --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "effector": "^20.12.1",
    "effector-react": "^20.6.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.12.19",
    "react-router-dom": "^5.1.2",
    "use-onclickoutside": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@typographist/postcss": "^6.1.1",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^25.1.0",
    "onchange": "^6.1.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "postcss-nested": "^4.2.1",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.2"
  }
}
